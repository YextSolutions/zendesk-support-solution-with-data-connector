{
  "$id": "c_Agent_Desktop_Searches_Clicks_by_Search_Vertical",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Searches \u0026 Clicks by Search Vertical",
  "description": "Answers Searches and Answers Clicks by Search Vertical",
  "query": {
    "metrics": [
      "yext/ANSWERS_SEARCHES",
      "yext/ANSWERS_CLICKS"
    ],
    "dimensions": [
      "yext/ANSWERS_SEARCH_VERTICAL"
    ],
    "filters": [
      {
        "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
        "in": [
          "PRODUCTION"
        ]
      },
      {
        "filter": "yext/ANSWERS_QUERY_SOURCE",
        "in": [
          "AGENT_DESKTOP"
        ]
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/ANSWERS_SEARCHES"
        },
        "descending": true
      }
    }
  }
}