{
  "$id": "c_Help_Site_Clicks_by_Search_Vertical",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Clicks by Search Vertical",
  "description": "Answers Clicks by Search Vertical",
  "query": {
    "metrics": [
      "yext/ANSWERS_CLICKS"
    ],
    "dimensions": [
      "yext/ANSWERS_SEARCH_VERTICAL"
    ],
    "filters": [
      {
        "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
        "in": [
          "PRODUCTION"
        ]
      },
      {
        "filter": "yext/ANSWERS_QUERY_SOURCE",
        "in": [
          "HELP_SITE"
        ]
      }
    ]
  },
  "display": {
    "pie": {
      "showLabels": true
    }
  }
}